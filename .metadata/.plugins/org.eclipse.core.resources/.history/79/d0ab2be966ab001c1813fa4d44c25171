import java.util.Locale;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		
		int A = sc.nextInt();
		int B = sc.nextInt();
		int C = sc.nextInt();
		
		int LARGER = (A + B + C + Math.abs(A - B - C)) / 2;
				
		System.out.println("LARGER " + LARGER + " eh o maior");
				
		sc.close();

	}

}
